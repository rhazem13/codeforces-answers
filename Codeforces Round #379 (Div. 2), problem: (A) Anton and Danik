#include <iostream>
#include <string>
using namespace std;
int main()
{
	int n;
	string s;
	cin >> n >> s;
	int A = 0, D = 0;
	for (int i = 0; i < n; i++) {
		if (s[i] == 'A') {
			A += 1;
		}
		else  {
			D += 1;
		}
		
	}
	if (A > D) {
		cout << "Anton";
	}
	else if(D>A) {
		cout << "Danik";
	}
	else {
		cout << "Friendship";
	}
}
